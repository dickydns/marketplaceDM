                    <ul class="product-list list-unstyled" >
  
                      <% for(var i=0; i < cart.length; i++) { %>
                        <% var allPrice = cart[i].price * cart[i].total;  %>
                            
                      <li class="product-list__item clearfix"> <a class="product-list__img" href="/detail/<%= cart[i].id_product %>"><img class="img-responsive" src="/images/<%= cart[i].picture %>" alt="Product"></a>
                        <div class="product-list__inner">
                          <h4 class="product-list__name"><a class="product-list__link" href="/detail/<%= cart[i].id_product %> "><span class="product-list__model"><%= cart[i].name %> </span><%= cart[i].category_name %></a></h4>
                          <span class="product-list__price">Rp. <%=  allPrice.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,") %></span> </div>
                          <label class="label label-default"><%= cart[i].total %></label>  
                          <a href="/add_cart/deleteCart/<%= cart[i].id_cart%>"> <i class="product-list__del icon icon-trash color_primary"></i>  </a>
                            
                      </li>
                      <% } %>
                      
                    </ul>
                    <div class="product-list__total">Subtotal:<span class="product-list__total_price">Rp. <% if(total[0].totalPrice === '0' || total[0].totalPrice === null){ } else{ %> <%= total[0].totalPrice.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,") %><% } %></span></div>
